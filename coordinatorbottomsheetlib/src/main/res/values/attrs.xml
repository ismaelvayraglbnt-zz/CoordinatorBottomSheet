<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomCollapsingToolbarLayout">
        <attr name="state" format="enum">
            <enum name="collapsed" value="0"/>
            <enum name="attached" value="1"/>
            <enum name="expanded" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="BottomCollapsibleActionBar">
        <attr name="anchor_point" format="dimension"/>
        <attr name="end_animation_point" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="BottomToolbar">
        <attr name="parallax_translation" format="dimension"/>
        <attr name="final_height" format="dimension"/>
    </declare-styleable>

    <item name="fake_toolbar" type="id"/>
    <item name="toolbar_bottom_panel" type="id"/>
    <item name="layoutSpace" type="id"/>
    <item name="toolbar_container" type="id"/>
</resources>
